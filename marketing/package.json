    "@prisma/client": "^5.16.2",
    "@prisma/instrumentation": "^5.20.0",
    "@types/swagger-ui-express": "^4.1.6",
    "amqplib": "^0.10.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "morgan": "^1.10.0",
    "prom-client": "^15.1.3",
    "swagger-autogen": "^2.23.7",
