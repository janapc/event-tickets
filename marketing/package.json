{
  "name": "marketing",
  "version": "1.0.0",
  "description": "api to manager leads",
  "main": "src/api.ts",
  "scripts": {
    "test": "jest",
    "linter": "eslint src",
    "generate-docs": "ts-node-dev src/infra/docs/index.ts",
    "dev:processor": "prisma migrate dev && ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/processor.ts",
    "dev:api": "prisma migrate dev && ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/api.ts",
    "build": "tsc -p .",
    "start:api": "prisma migrate deploy  && node -r ts-node/register/transpile-only -r tsconfig-paths/register build/api.js",
    "start:processor": "prisma migrate deploy  && node -r ts-node/register/transpile-only -r tsconfig-paths/register build/processor.js"
  },
